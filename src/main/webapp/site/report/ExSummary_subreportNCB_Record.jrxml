	<queryString>
		<![CDATA[SELECT
     WRK_NCB."NCB_CUS_NAME" AS NCB_CUS_NAME,
     WRK_NCB."CHECK_IN_6_MONTH" AS CHECK_IN,
     WRK_NCB."CURRENT_PAYMENT_TYPE" AS CURRENT_PAYMENT_TYPE,
     WRK_NCB."HISTORY_PAYMENT_TYPE" AS HISTORY_PAYMENT_TYPE,
     WRK_NCB."NPL_FLAG" AS NPL_FLAG,
     WRK_NCB."TDR_FLAG" AS TDR_FLAG,
     MST_TDR_CONDITION."DESCRIPTION" AS DESCRIPTION,
     WRK_NCB."PAYMENT_CLASS" AS PAYMENT_CLASS,
     WRK_CUSTOMER."NAME_TH" AS WRK_CUSTOMER_NAME_TH,
     MST_TITLE."TITLE_TH" AS MST_TITLE_TITLE_TH,
     WRK_CUSTOMER."LASTNAME_TH" AS WRK_CUSTOMER_LASTNAME_TH
FROM
     "SLOS"."WRK_NCB" WRK_NCB LEFT OUTER JOIN "SLOS"."MST_TDR_CONDITION" MST_TDR_CONDITION ON WRK_NCB."TDR_CONDITION_ID" = MST_TDR_CONDITION."ID"
     INNER JOIN "SLOS"."WRK_CUSTOMER" WRK_CUSTOMER ON WRK_NCB."CUSTOMER_ID" = WRK_CUSTOMER."ID"
     INNER JOIN "SLOS"."MST_TITLE" MST_TITLE ON WRK_CUSTOMER."TITLE_ID" = MST_TITLE."ID"
WHERE
     WRK_CUSTOMER."WORKCASE_ID" = 147]]>
