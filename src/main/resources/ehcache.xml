<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="ehcache.xsd"
         updateCheck="false" monitoring="autodetect"
         dynamicConfig="false"
         maxBytesLocalHeap="20M" >

    <defaultCache memoryStoreEvictionPolicy="LRU"
                  eternal="false"
                  statistics="true">
        <persistence strategy="none"/>
    </defaultCache>

    <cache name="org.hibernate.cache.internal.StandardQueryCache"
           maxBytesLocalHeap="50M"
           eternal="false"
           overflowToDisk="false"
           statistics="true"
            />

    <cache name="org.hibernate.cache.spi.UpdateTimestampsCache"
           maxBytesLocalHeap="100K"
           eternal="false"
           overflowToDisk="false"
           statistics="true"
            />

    <cache name="com.clevel.selos.model.db.master.BusinessGroup"
           maxBytesLocalHeap="100K"
           eternal="true"
           overflowToDisk="false"
           statistics="true"
            />

    <cache name="com.clevel.selos.model.db.master.BusinessDescription"
           maxBytesLocalHeap="100K"
           eternal="true"
           overflowToDisk="false"
           statistics="true"
            />

    <cache name="com.clevel.selos.model.db.master.Country"
           maxBytesLocalHeap="200K"
           eternal="true"
           overflowToDisk="false"
           statistics="true"
            />

    <cache name="com.clevel.selos.model.db.master.Region"
           maxBytesLocalHeap="5K"
           eternal="true"
           overflowToDisk="false"
           statistics="true"
            />

    <cache name="com.clevel.selos.model.db.master.Province"
           maxBytesLocalHeap="40K"
           eternal="true"
           overflowToDisk="false"
           statistics="true"
            />

    <cache name="com.clevel.selos.model.db.master.District"
           maxBytesLocalHeap="420K"
           eternal="true"
           overflowToDisk="false"
           statistics="true"
            />

    <cache name="com.clevel.selos.model.db.master.SubDistrict"
           maxBytesLocalHeap="4M"
           eternal="true"
           overflowToDisk="false"
           statistics="true"
            />

</ehcache>
